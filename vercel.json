{
    "functions": {
        "api/index.php":{
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes":[
        {
            "src": "api/index.php",
            "dest": "api/index.php"
        },
        {
            "src": "/images/(.*).png",
            "dest": "/public/images/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}